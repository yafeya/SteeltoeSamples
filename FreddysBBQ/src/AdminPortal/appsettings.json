{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "spring": {
    "application": {
      "name": "admin-portal"
    },
    "cloud": {
      "config": {
        "uri": "http://localhost:8888",
        "validate_certificates": false
      }
    }
  },
  "eureka": {
    "client": {
      "validateCertificates": false
    },
    "instance": {
      "port": 8000
    }
  },
  "mysql": {
    "client": {
      "server": "localhost"
    }
  },
  "security": {
    "oauth2": {
      "client": {
        "additionalScopes": "menu.read menu.write order.admin order.me",
        "saveTokens": true,
        "validateCertificates": false
      }
    }
  },
  "Branding": {
    "RestaurantName": "Freddy's BBQ Joint",
    "MenuTitle": "Freddy's Menu"
  }
}


