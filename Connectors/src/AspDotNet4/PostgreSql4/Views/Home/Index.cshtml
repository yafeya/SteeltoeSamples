@model Dictionary<string, string>
@{
    ViewBag.Title = "Steeltoe PostgreSQL Sample";
}

<h1>@ViewBag.Title</h1>
<h4>Data stored in PostgreSQL:</h4>
@foreach (var item in Model)
{
    <h5>Key @item.Key = @item.Value</h5>
}
<h4>Server Info:</h4>
<h5>Database: @ViewBag.Database</h5>
@{ #if DEBUG }
<h5>DataSource: @Html.ViewBag.DataSource</h5>
@{ #endif }