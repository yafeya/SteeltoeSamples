{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    },
    "Console": {
      "DisableColors": true
    }
  },
  "AllowedHosts": "*",
  "spring": {
    "application": {
      "name": "fortuneService"
    }
  },
  "eureka": {
    "client": {
      "serviceUrl": "http://eureka:8761/eureka/",
      "shouldFetchRegistry": false,
      "ValidateCertificates": false
    },
    "instance": {
      "port": 5000
    }
  }
}

