{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Warning",
      "Pivotal": "Information",
      "Steeltoe": "Information"
    },
      "Console": {
        "DisableColors": true
      }
    },
    "management": {
      "endpoints": {
        "path": "/cloudfoundryapplication",
        "cloudfoundry": {
          "validateCertificates": false
        }
      },
      "metrics": {
        "exporter": {
          "cloudfoundry": {
            "validateCertificates": false
          }
        }
      }
    },
    "info": {
      "appsetting1": "appsetting1",
      "appsetting2": "appsetting2",
      "appsetting3": "appsetting3",
      "appsetting4": "appsetting4"
    },
    "mysql": {
      "client": {
        "sslmode": "none"
      }
    }
  }
